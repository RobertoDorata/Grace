<resources>

    <style name="MyActionBar" parent="Base.Theme.MaterialComponents.Light">
        <item name="android:background">@color/Background</item>
        <item name="android:textColor">@color/Primary</item>
    </style>

    <style name="MyActionBar.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="MyActionBar.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="MyActionBar.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="UnelevatedButton" parent="Widget.MaterialComponents.Button.UnelevatedButton">
    </style>

</resources>
